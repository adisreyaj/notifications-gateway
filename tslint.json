{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [false],
        "interface-name": [false],
        "no-console": [false],
        "quotemark": [true],
        "object-literal-sort-keys": [false],
        "max-line-length": [true, 180],
        "arrow-parens": false
    },
    "rulesDirectory": []
}